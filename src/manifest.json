{
	"manifest_version": 2,
	"name": "COVID-19 ȘTIRI OFICIALE",
	"version": "1.0.0",
	"description": "Informații despre Coronavirus (COVID-19)",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": [
				"dependencies/popper.js",
